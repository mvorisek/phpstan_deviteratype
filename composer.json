{
    "name": "iterphpstan/iterphpstan",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "atk4/ui": "dev-develop",
        "jdorn/sql-formatter": "^1.2.17",
        "mahalux/atk4-hintable": "dev-develop as 1.9999"
    },
    "require-dev": {
        "behat/behat": "^3.4",
        "behat/mink-extension": "^2.2",
        "behat/mink-selenium2-driver": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9",
        "guzzlehttp/guzzle": "^6.3",
        "johnkary/phpunit-speedtrap": "^3.2",
        "phpstan/phpstan": "^0.12.58",
        "phpunit/phpunit": "^9.3",
        "symfony/contracts": ">=2.1",
        "symfony/process": "^5.0"
    },
    "conflict": {
        "doctrine/dbal": ">=3"
    },
    "autoload": {
        "psr-4": {
            "Maha\\Atk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atk4\\Core\\Tests\\": "vendor/atk4/core/tests/",
            "Atk4\\Data\\Tests\\": "vendor/atk4/data/tests/",
            "Atk4\\Ui\\Tests\\": "vendor/atk4/ui/tests/",
            "Atk4\\Ui\\Behat\\": "vendor/atk4/ui/tests-behat/bootstrap/",
            "Atk4\\Dsql\\Tests\\": "vendor/atk4/dsql/tests/",
            "Atk4\\Schema\\Tests\\": "vendor/atk4/data/tests-schema/",
            "Mvorisek\\Atk4\\Hintable\\Tests\\": "vendor/mahalux/atk4-hintable/tests/"
        }
    }
}
